				{
					"fluid": {
						"max": "1.414rem",
						"min": "1.25rem"
					},
					"name": "Large",
					"size": "1.414rem",
					"slug": "large"
				},
				{
					"fluid": {
						"max": "3.998rem",
						"min": "2.5rem"
