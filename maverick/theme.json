				{
					"fontFace": [
						{
							"fontFamily": "New Century Schoolbook",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/new-century-schoolbook_normal_400.ttf"
							]
						},
						{
							"fontFamily": "New Century Schoolbook",
							"fontStyle": "italic",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/new-century-schoolbook_italic_400.ttf"
							]
