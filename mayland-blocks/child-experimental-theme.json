				"width": {
					"default": "750px",
					"wide": "1022px"
				},
				"button": {
					"color": {
						"hoverBackground": "var(--wp--custom--color--tertiary)"
					}
				}
			}
		}
	},
	"styles": {
		"root": {
			"color": {
				"background": "var(--wp--custom--color--background)",
