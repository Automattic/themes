		tinyMCEPreInit = {
			baseURL: "https://s0.wp.com/wp-includes/js/tinymce",
			suffix: ".min",
						mceInit: {},
			qtInit: {'replycontent':{id:"replycontent",buttons:"strong,em,link,block,del,ins,img,ul,ol,li,code,close"}},
