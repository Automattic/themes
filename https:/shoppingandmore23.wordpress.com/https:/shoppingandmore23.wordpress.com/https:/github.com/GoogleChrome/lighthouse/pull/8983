			<script type="text/javascript">
	if ( typeof jQuery !== 'undefined' ) {
		jQuery( document ).ready( function($) {
			// track how often file details (title, caption, etc) are changed
			$( 'body' ).on( 'change', '.media-sidebar input, .media-sidebar textarea, .media-sidebar select', function( event ) {
				var setting,
					node = event.target,
					target = $( node );

				if ( 'SELECT' === node.nodeName ) {
					setting = target.data( 'setting' );
				} else {
					setting = target.parent().data( 'setting' );
				}

				if ( setting ) {
					new Image().src = '//pixel.wp.com/g.gif?v=wpcom-no-pv&x_edit-file-details=' + setting + '&rand=' + Math.random();
				}
			} );

			// track how often the date picker is used
			$( 'body' ).on( 'change', '#media-attachment-date-filters', function( event ) {
				var selection;
				if ( $( '#media-attachment-date-filters' ).val() >= 0 ) {
					selection = 'month';
				} else {
					selection = 'all';
				}

				if ( selection ) {
					new Image().src = '//pixel.wp.com/g.gif?v=wpcom-no-pv&x_media-date-picker=' + selection + '&rand=' + Math.random();
				}
			} );

					} );
	}
	</script><script type="text/javascript">
jQuery( function() {
	( 'undefined' !== typeof tinymce ) && tinymce.on( 'AddEditor', function( event ) {
		event.editor.on( 'BeforeSetContent', function( event ) {
			var editor = event.target;
			Object.keys( editor.schema.elements ).forEach( function( key, index ) {
				editor.schema.elements[ key ].attributes['markdown'] = {};
				editor.schema.elements[ key ].attributesOrder.push( 'markdown' );
			} );
		} );
	}, true );
} );
</script>

