{
	"settings": {
		"blocks": {
			"core/spacer": {
				"color": {
					"palette": [
						{
							"color": "#111111",
							"name": "Primary",
							"slug": "primary"
						},
						{
							"color": "#f7f8f2",
							"name": "Secondary",
							"slug": "secondary"
						},
						{
							"color": "#f7f8f2",
							"name": "Foreground",
							"slug": "foreground"
						},
						{
							"color": "#030201",
							"name": "Background",
							"slug": "background"
						},
						{
							"color": "#f7f8f280",
							"name": "Tertiary",
							"slug": "tertiary"
						}
					]
				}
			}
		},
		"color": {
			"palette": [
				{
					"color": "#333333",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#d9d9d9",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "#d9d9d9",
					"name": "Foreground",
					"slug": "foreground"
				},
				{
					"color": "#333333",
					"name": "Background",
					"slug": "background"
				},
				{
					"color": "#d9d9d980",
					"name": "Tertiary",
					"slug": "tertiary"
				}
			]
		},
		"layout": {
			"contentSize": "900px",
			"wideSize": "1200px"
		}
	},
	"styles": {
		"blocks": {
			"core/avatar": {
				"filter": {
					"duotone": "var(--wp--preset--duotone--dust)"
				}
			},
			"core/comment-date": {
				"typography": {
					"fontSize": "0.8rem",
					"letterSpacing": "0.02rem"
				}
			},
			"core/comment-edit-link": {
				"typography": {
					"fontSize": "0.8rem",
					"letterSpacing": "0.07rem",
					"textTransform": "uppercase"
				}
			},
			"core/comment-reply-link": {
				"typography": {
					"fontSize": "0.8rem",
					"letterSpacing": "0.04rem",
					"textTransform": "uppercase"
				}
			},
			"core/comments": {
				"spacing": {
					"padding": {
						"top": "2rem"
					}
				}
			},
			"core/cover": {
				"color": {
					"text": "var(--wp--preset--color--secondary)"
				},
				"elements": {
					"heading": {
						"color": {
							"background": "var(--wp--preset--color--primary)",
							"text": "var(--wp--preset--color--secondary)"
						}
					},
					"link": {
						"color": {
							"text": "var(--wp--preset--color--secondary)"
						}
					}
				},
				"filter": {
					"duotone": "var(--wp--preset--duotone--dust)"
				},
				"typography": {
					"textTransform": "uppercase"
				}
			},
			"core/navigation": {
				"spacing": {
					"blockGap": "1.5rem"
				},
				"typography": {
					"textTransform": "uppercase"
				}
			},
			"core/post-author": {
				"typography": {
					"lineHeight": "1"
				}
			},
			"core/post-comments-form": {
				"spacing": {
					"margin": {
						"bottom": "0rem",
						"left": "0rem",
						"right": "0rem",
						"top": "4rem"
					}
				}
			},
			"core/post-content": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"core/post-date": {
				"typography": {
					"lineHeight": "1"
				}
			},
			"core/post-excerpt": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/post-featured-image": {
				"filter": {
					"duotone": "var(--wp--preset--duotone--dust)"
				}
			},
			"core/query-pagination": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)",
					"lineHeight": "1"
				}
			},
			"core/query-title": {
				"typography": {
					"textTransform": "uppercase"
				}
			},
			"core/search": {
				"border": {
					"bottom": {
						"style": "none",
						"width": "0px"
					},
					"left": {
						"style": "none",
						"width": "0px"
					},
					"right": {
						"style": "none",
						"width": "0px"
					},
					"top": {
						"style": "none",
						"width": "0px"
					}
				}
			},
			"core/site-title": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)",
					"textTransform": "uppercase"
				}
			}
		},
		"color": {
			"text": "var(--wp--preset--color--secondary)"
		},
		"elements": {
			"button": {
				"color": {
					"background": "var(--wp--preset--color--secondary)"
				}
			},
			"h1": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--d-din-exp)",
					"letterSpacing": "0rem",
					"lineHeight": "1.1",
					"textTransform": "none"
				}
			},
			"h2": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--d-din-exp)",
					"fontStyle": "normal",
					"fontWeight": "400",
					"letterSpacing": "0.01rem"
				}
			},
			"h3": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--d-din-exp)",
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontStyle": "normal",
					"fontWeight": "400",
					"letterSpacing": "0.01rem",
					"textTransform": "none"
				}
			},
			"h4": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--d-din-exp)",
					"fontSize": "var(--wp--preset--font-size--small)",
					"letterSpacing": "0.01rem",
					"textTransform": "uppercase"
				}
			},
			"h5": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--d-din-exp)",
					"letterSpacing": "0.01rem"
				}
			},
			"h6": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--d-din-exp)",
					"fontSize": "0.8rem",
					"letterSpacing": "0.02rem",
					"textTransform": "uppercase"
				}
			},
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--d-din-exp)"
				}
			},
			"link": {
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--tertiary)"
					}
				},
				"color": {
					"text": "var(--wp--preset--color--secondary)"
				}
			}
		},
		"spacing": {
			"padding": {
				"bottom": "0rem",
				"left": "1.5rem",
				"right": "1.5rem",
				"top": "0rem"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--barlow)",
			"fontSize": "var(--wp--preset--font-size--small)",
			"letterSpacing": "0.01rem"
		}
	},
	"title": "Charcoal",
	"version": 2,
	"$schema": "https://schemas.wp.org/trunk/theme.json"
}