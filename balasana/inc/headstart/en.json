            "post_content": "<!-- wp:spacer {\"height\":32} -->\n<div style=\"height:32px;\" aria-hidden=\"true\" class=\"wp-block-spacer\"><\/div>\n<!-- \/wp:spacer -->\n\n<!-- wp:paragraph -->\n<p>Do you have questions about our yoga studio or the yoga classes we offer here?<\/p>\n<!-- \/wp:paragraph -->\n\n<!-- wp:paragraph -->\n<p>We\u2019re always happy to answer your questions or receive your comments about how we can improve our service.<\/p>\n<!-- \/wp:paragraph -->\n\n<!-- wp:spacer {\"height\":32} -->\n<div style=\"height:32px;\" aria-hidden=\"true\" class=\"wp-block-spacer\"><\/div>\n<!-- \/wp:spacer -->\n\n<!-- wp:columns {\"align\":\"wide\",\"className\":\"has-2-columns\"} -->\n<div class=\"wp-block-columns alignwide has-2-columns\"><!-- wp:column -->\n<div class=\"wp-block-column\"><!-- wp:heading {\"align\":\"left\",\"level\":4} -->\n<h4 class=\"has-text-align-left\">Get in Touch<\/h4>\n<!-- \/wp:heading -->\n\n<!-- wp:jetpack\/contact-info -->\n<div class=\"wp-block-jetpack-contact-info\"><!-- wp:jetpack\/email {\"email\":\"mail@example.com\"} -->\n<div class=\"wp-block-jetpack-email\"><a href=\"mailto:mail@example.com\">mail@example.com<\/a><\/div>\n<!-- \/wp:jetpack\/email -->\n\n<!-- wp:jetpack\/phone {\"phone\":\"(123) 456-7890\"} -->\n<div class=\"wp-block-jetpack-phone\"><a href=\"tel:1234567890\">(123) 456-7890<\/a><\/div>\n<!-- \/wp:jetpack\/phone -->\n\n<!-- wp:jetpack\/address {\"address\":\"99 Wythe Avenue\",\"city\":\"Brooklyn\",\"region\":\"NY\",\"postal\":\"11249\",\"linkToGoogleMaps\":true} -->\n<div class=\"wp-block-jetpack-address\"><a href=\"https:\/\/www.google.com\/maps\/search\/99+Wythe Avenue,+Brooklyn,+NY,+11249\" target=\"_blank\" rel=\"noopener noreferrer\" title=\"Open address in Google Maps\"><div class=\"jetpack-address__address jetpack-address__address1\">99 Wythe Avenue<\/div><div><span class=\"jetpack-address__city\">Brooklyn<\/span>, <span class=\"jetpack-address__region\">NY<\/span> <span class=\"jetpack-address__post