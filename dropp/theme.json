		"blocks": {
			"core/button": {
				"border": {
					"radius": "0px"
				},
				"color": {
					"background": "var(--wp--preset--color--secondary)"
				},
				"variations": {
					"outline": {
						"border": {
							"bottom": {
								"color": "var(--wp--preset--color--contrast)",
								"style": "solid",
								"width": "1px"
							},
							"left": {
								"color": "var(--wp--preset--color--contrast)",
								"style": "solid",
								"width": "1px"
							},
							"right": {
								"color": "var(--wp--preset--color--contrast)",
								"style": "solid",
								"width": "1px"
							},
							"top": {
								"color": "var(--wp--preset--color--contrast)",
								"style": "solid",
								"width": "1px"
							}
						}
					}
				}
			},
			"core/code": {
				"border": {
					"bottom": {
						"style": "none",
						"width": "0px"
					},
					"color": "var(--wp--preset--color--contrast)",
					"left": {
						"style": "none",
						"width": "0px"
					},
					"radius": "0.25rem",
					"right": {
						"style": "none",
						"width": "0px"
					},
					"style": "solid",
					"top": {
