{
	"$schema": "https://json.schemastore.org/theme-v1.json",
	"version": 1,
	"settings": {
		"color": {
			"palette": [
				{
					"slug": "primary",
					"color": "#FFFFFF",
					"name": "Primary"
				},
				{
					"slug": "secondary",
					"color": "#FEF6B2",
					"name": "Secondary"
				},
				{
					"slug": "background",
					"color": "#192833",
					"name": "Background"
				},
				{
					"slug": "tertiary",
					"color": "#000000",
					"name": "Tertiary"
				}
			]
		},
		"custom": {
			"heading": {
				"typography": {
					"fontWeight": "300"
				}
			}
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFamily": "'Rubik', sans-serif",
					"fontSlug": "rubik",
					"slug": "body-font",
					"name": "Body (Rubik)",
					"google": "family=Rubik:ital,wght@0,300..900;1,300..900"
				},
				{
					"fontFamily": "'Rubik', sans-serif",
					"fontSlug": "rubik",
					"slug": "heading-font",
					"name": "Headings (Rubik)",
					"google": "family=Rubik:ital,wght@0,300..900;1,300..900"
				}
			]
		}
	},
	"styles": {
		"blocks": {
			"core/site-title": {
				"typography": {
					"fontWeight": "normal"
				}
			}
		},
		"elements": {
			"link": {
				"color": {
					"text": "var(--wp--custom--color--secondary)"
				}
			}
		}
	}
}
