						"typography": {
							"textDecoration": "none"
						}
					}
				},
				"typography": {
					"fontSize": "var:preset|font-size|medium",
					"textTransform": "uppercase"
				}
			},
			"core/paragraph": {
				"typography": {
					"letterSpacing": "0.1em"
				}
			},
			"core/post-author": {
				"typography": {
					"fontSize": "var:preset|font-size|medium",
