					"fontWeight": "300",
					"letterSpacing": "0.05em",
					"lineHeight": "1.4"
				}
			},
			"core/query-pagination": {
				"typography": {
					"fontSize": "1.5rem",
					"textTransform":"uppercase",
					"fontStyle":"normal",
					"fontWeight":"600"
				}
			},
			"core/quote": {
				"border": {
					"color": "var:preset|color|primary",
					"style": "solid",
					"width": "0 0 0 1px"
