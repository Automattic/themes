{
  "name": "blockbase",
  "version": "1.0.1",
  "description": "Blank Parent Theme",
  "bugs": {
    "url": "https://github.com/Automattic/themes/issues"
  },
  "homepage": "https://github.com/Automattic/themes#readme",
  "devDependencies": {
    "@wordpress/base-styles": "^3.3.0",
    "@wordpress/browserslist-config": "^2.2.2",
    "chokidar-cli": "^2.1.0",
    "deepmerge": "^4.2.2",
    "node-sass": "^4.13.1",
    "node-sass-package-importer": "^5.3.2",
    "npm-run-all": "^4.1.5",
    "rtlcss": "^2.4.0"
  },
  "rtlcssConfig": {
    "options": {
      "autoRename": false,
      "autoRenameStrict": false,
      "blacklist": {},
      "clean": true,
      "greedy": false,
      "processUrls": false,
      "stringMap": []
    },
    "plugins": [],
    "map": false
  },
  "browserslist": [
    "extends @wordpress/browserslist-config"
  ],
  "scripts": {
    "start": "chokidar \"sass/**/*.scss\" -c \"npm run build\" --initial",
    "start:child": "run-p start \"build:child -- {@} watch\" --",
    "start:all": "chokidar \"sass/**/*.scss\" \"experimental-theme.json\" -c \"npm run build:all\" --initial",
    "buildBlock:button": "node-sass --importer node_modules/node-sass-package-importer/dist/cli.js sass/blocks/_button.scss assets/blocks/button.css --output-style expanded",
    "buildBlock:code": "node-sass --importer node_modules/node-sass-package-importer/dist/cli.js sass/blocks/_code.scss assets/blocks/code.css --output-style expanded",
    "buildBlock:columns": "node-sass --importer node_modules/node-sass-package-importer/dist/cli.js sass/blocks/_columns.scss assets/blocks/columns.css --output-style expanded",
    "buildBlock:file": "node-sass --importer node_modules/node-sass-package-importer/dist/cli.js sass/blocks/_file.scss assets/blocks/file.css --output-style expanded",
    "buildBlock:gallery": "node-sass --importer node_modules/node-sass-package-importer/dist/cli.js sass/blocks/_gallery.scss assets/blocks/gallery.css --output-style expanded",
    "buildBlock:image": "node-sass --importer node_modules/node-sass-package-importer/dist/cli.js sass/blocks/_image.scss assets/blocks/image.css --output-style expanded",
    "buildBlock:navigation": "node-sass --importer node_modules/node-sass-package-importer/dist/cli.js sass/blocks/_navigation.scss assets/blocks/navigation.css --output-style expanded",
    "buildBlock:paragraph": "node-sass --importer node_modules/node-sass-package-importer/dist/cli.js sass/blocks/_paragraph.scss assets/blocks/paragraph.css --output-style expanded",
    "buildBlock:post-author": "node-sass --importer node_modules/node-sass-package-importer/dist/cli.js sass/blocks/_post-author.scss assets/blocks/post-author.css --output-style expanded",
    "buildBlock:post-comments": "node-sass --importer node_modules/node-sass-package-importer/dist/cli.js sass/blocks/_post-comments.scss assets/blocks/post-comments.css --output-style expanded",
    "buildBlock:pullquote": "node-sass --importer node_modules/node-sass-package-importer/dist/cli.js sass/blocks/_pullquote.scss assets/blocks/pullquote.css --output-style expanded",
    "buildBlock:query-pagination": "node-sass --importer node_modules/node-sass-package-importer/dist/cli.js sass/blocks/_query-pagination.scss assets/blocks/query-pagination.css --output-style expanded",
    "buildBlock:quote": "node-sass --importer node_modules/node-sass-package-importer/dist/cli.js sass/blocks/_quote.scss assets/blocks/quote.css --output-style expanded",
    "buildBlock:search": "node-sass --importer node_modules/node-sass-package-importer/dist/cli.js sass/blocks/_search.scss assets/blocks/search.css --output-style expanded",
    "buildBlock:separator": "node-sass --importer node_modules/node-sass-package-importer/dist/cli.js sass/blocks/_separator.scss assets/blocks/separator.css --output-style expanded",
    "buildBlock:table": "node-sass --importer node_modules/node-sass-package-importer/dist/cli.js sass/blocks/_table.scss assets/blocks/table.css --output-style expanded",
    "buildBlock:video": "node-sass --importer node_modules/node-sass-package-importer/dist/cli.js sass/blocks/_video.scss assets/blocks/video.css --output-style expanded",
    "buildBlocks": "npm run buildBlock:button && npm run buildBlock:code && npm run buildBlock:columns && npm run buildBlock:file && npm run buildBlock:gallery && npm run buildBlock:image && npm run buildBlock:navigation && npm run buildBlock:paragraph && npm run buildBlock:post-author && npm run buildBlock:post-comments && npm run buildBlock:pullquote && npm run buildBlock:query-pagination && npm run buildBlock:quote && npm run buildBlock:search && npm run buildBlock:separator && npm run buildBlock:table && npm run buildBlock:video",
    "build": "npm run buildBlocks && node-sass --importer node_modules/node-sass-package-importer/dist/cli.js sass/ponyfill.scss assets/ponyfill.css --output-style expanded --indent-type tab --indent-width 1 --source-map true",
    "build:child": "node build.js",
    "build:all": "npm run build && sh ./rebuild-child-themes.sh"
  }
}
