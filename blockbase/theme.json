		{
			"name": "blank",
			"title": "Blank",
			"postTypes": [
				"page",
				"post"
			]
		},
		{
			"name": "header-footer-only",
			"title": "Header and Footer Only",
			"postTypes": [
				"page",
				"post"
			]
		},
		{
			"name": "footer-only",
			"title": "Footer Only",
			"postTypes": [
				"page",
				"post"
			]
		}
