{
	"settings": {
		"color": {
			"palette": [
				{
					"color": "#250a76",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#fafdf1",
					"name": "Contrast",
					"slug": "contrast"
				},
				{
					"color": "#cf9057",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#cf9057",
					"name": "Secondary",
					"slug": "secondary"
				}
			]
		}
	},
	"styles": {
		"blocks": {
			"core/image": {
				"filter": {
					"duotone": "var(--wp--preset--duotone--duo-one)"
				}
			},
			"core/post-featured-image": {
				"filter": {
					"duotone": "var(--wp--preset--duotone--duo-one)"
				}
			},
			"core/site-title": {
				"color": {
					"text": "var(--wp--preset--color--primary)"
				},
				"elements": {
					"link": {
						":hover": {
							"color": {
								"text": "var(--wp--preset--color--contrast)"
							}
						},
						"color": {
							"text": "var(--wp--preset--color--primary)"
						}
					}
				}
			}
		},
		"color": {
			"text": "var(--wp--preset--color--primary)"
		},
		"elements": {
			"button": {
				"color": {
					"background": "var(--wp--preset--color--primary)"
				}
			},
			"caption": {
				"color": {
					"text": "var(--wp--preset--color--primary)"
				}
			},
			"link": {
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--contrast)"
					}
				},
				"color": {
					"text": "var(--wp--preset--color--primary)"
				}
			}
		}
	},
	"title": "Orbital",
	"version": 2,
	"$schema": "https://schemas.wp.org/trunk/theme.json"
