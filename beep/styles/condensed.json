{
	"settings": {
		"appearanceTools": true,
		"color": {
			"palette": [
				{
					"color": "#00E49A",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#FB6500",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "#FBC300",
					"name": "Tertiary",
					"slug": "tertiary"
				},
				{
					"color": "#3B3B3B",
					"name": "Inactive",
					"slug": "inactive"
				},
				{
					"color": "#ffffff",
					"name": "White",
					"slug": "white"
				}
			]
		},
		"layout": {
			"contentSize": "80vw",
			"wideSize": "92vw"
		},
		"spacing": {
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
