			"customFontSize": true,
			"defaultFontSizes": false,
			"dropCap": true,
			"fluid": true,
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontFamily": "Open Runde",
							"fontStyle": "normal",
							"fontWeight": "600",
							"src": [
								"file:./assets/fonts/open-runde/open-runde_normal_600.woff2"
							]
						},
						{
							"fontFamily": "Open Runde",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/open-runde/open-runde_normal_700.woff2"
							]
						}
					],
					"fontFamily": "Open Runde",
					"slug": "open-runde"
				}
			],
			"fontSizes": [
				{
					"fluid": {
						"max": "1.0625rem",
						"min": "0.825rem"
