{
	"settings": {
		"appearanceTools": true,
		"color": {
			"palette": [
				{
					"color": "#edeeef",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#030202",
					"name": "Contrast",
					"slug": "contrast"
				},
				{
					"color": "#030202",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#ffffff",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "#282828",
					"name": "Tertiary",
					"slug": "tertiary"
				}
