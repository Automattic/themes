						"color": "#FFF9F5",
						"name": "Background"
					},
					{
						"slug": "link",
						"color": "#3454FF",
						"name": "Link"
					},
					{
						"slug": "selection",
						"color": "#CCCCCC",
						"name": "Selection"
					}
				]
			}
		},
		"defaults": {
			"color": {
				"palette": [
					{
						"slug": "blue",
						"color": "#3454FF",
						"name": "Blue"
					},
					{
						"slug": "darker-blue",
						"color": "#293DAC",
						"name": "Darker Blue"
					},
					{
						"slug": "black",
						"color": "#131313",
						"name": "Black"
					},
					{
						"slug": "almost-white",
						"color": "#FFF9F5",
						"name": "Almost White"
					},
					{
						"slug": "gray",
						"color": "#CCCCCC",
						"name": "Gray"
					}
				]
			},
			"typography": {
				"customFontSize": true,
				"customLineHeight": true,
				"fontSizes": [
					{
						"name": "Extra Small",
						"size": "9px",
						"slug": "x-small"
					},
					{
						"name": "Small",
						"size": "16.6px",
						"slug": "small"
					},
					{
						"name": "Normal",
						"size": "20px",
						"slug": "normal"
					},
					{
						"name": "Large",
						"size": "24px",
						"slug": "large"
					},
					{
						"name": "Huge",
						"size": "32px",
						"slug": "huge"
					},
					{
						"name": "Giant",
						"size": "88px",
						"slug": "giant"
					}
	
				],
				"fontFamilies": [
					{
						"fontFamily": "var(--font-headings),'Work Sans', Helvetica, Arial, sans-serif",
						"slug": "headings",
						"name": "Headings"
					},
					{
						"fontFamily": "var(--font-base),'Work Sans', Helvetica, Arial, sans-serif",
						"slug": "base",
						"name": "Base"
					}
				]
			},
			"spacing": {
				"customPadding": true,
				"units": [ "px", "em", "rem", "vh", "vw" ]
			},
			"custom": {
				"line-height": {
					"body": 1.6,
					"headings": 1.125
				},
				"margin": {
					"horizontal": "20px",
					"vertical": "20px"
				},
				"width": {
					"default": "750px",
					"wide": "1200px"
				},
				"button": {
					"font-weight": "normal",
					"font-family": "var(--wp--preset--font-family--base)",
					"font-size": "var(--wp--preset--font-size--normal)",
					"border-radius": "0px",
					"color": {
						"hover-text": "var(--wp--preset--color--background)",
						"hover-background": "var(--wp--preset--color--secondary)"
					}
				},
				"form": {
					"padding": "5px",
					"border": {
						"radius": "0",
						"color": "#cccccc",
						"width": "2px",
						"style": "solid"
					},
					"color": {
						"text": "var(--wp--preset--color--foreground)",
						"background": "transparent",
						"box-shadow": "none"
					}
				},
				"navigation": {
					"padding": "10px",
					"color": {
						"text": "var(--wp--preset--color--foreground)",
						"background": "var(--wp--preset--color--background)",
						"hover-text": "var(--wp--preset--color--secondary)",
						"hover-background": "var(--wp--preset--color--background)"
					},
					"submenu": {
						"padding": "10px",
						"border": {
							"radius": "0",
							"color": "var(--wp--preset--color--foreground)",
							"width": "1px",
							"style": "solid"
						},
						"color": {
							"text": "var(--wp--preset--color--foreground)",
							"background": "var(--wp--preset--color--background)",
							"hover-text": "var(--wp--preset--color--background)",
							"hover-background": "var(--wp--preset--color--foreground)"
						}
					}
				}
			}
		}
	},
	"styles": {
		"root": {
			"color": {
				"background": "var(--wp--preset--color--background)",
				"text": "var(--wp--preset--color--foreground)",
				"link": "var(--wp--preset--color--foreground)"
			},
			"typography": {
				"fontSize": "var(--wp--preset--font-size--normal)",
				"lineHeight": "var(--wp--custom--line-height--body)",
				"fontFamily": "var(--wp--preset--font-family--base)",
				"fontWeight": "400"
			}
		},
		"core/button": {
			"color": {
				"text": "var(--wp--preset--color--background)",
				"background": "var(--wp--preset--color--primary)"
			}
		},
		"core/heading/h1": {
			"typography": {
				"fontSize": "80px",
				"fontWeight": "300",
				"lineHeight": "var(--wp--custom--line-height--headings)"
			}
		},
		"core/heading/h2": {
			"typography": {
				"fontSize": "52px",
				"lineHeight": "var(--wp--custom--line-height--headings)"
			}
		},
		"core/heading/h3": {
			"typography": {
				"fontSize": "32px",
				"lineHeight": "var(--wp--custom--line-height--headings)"
			}
		},
		"core/heading/h4": {
			"typography": {
				"fontSize": "32px",
				"lineHeight": "var(--wp--custom--line-height--headings)"
			}
		},
		"core/heading/h5": {
			"typography": {
				"fontSize": "24px",
				"lineHeight": "var(--wp--custom--line-height--headings)"
			}
		},
		"core/heading/h6": {
			"typography": {
				"fontSize": "20px",
				"lineHeight": "var(--wp--custom--line-height--headings)"
			}
		},
		"core/post-title/h1": {
			"typography": {
				"fontSize": "var(--wp--preset--font-size--huge)",
				"lineHeight": "var(--wp--custom--line-height--headings)"
			}
		},
		"core/post-date": {
			"color": {
				"text": "var(--wp--preset--color--foreground)",
				"link": "var(--wp--preset--color--foreground)"
			},
			"typography": {
				"fontSize": "var(--wp--preset--font-size--small)"
			}
		},
		"core/site-title": {
			"typography": {
				"fontSize": "var(--wp--preset--font-size--large)"
			}
		},
		"core/navigation": {
			"typography": {
				"fontSize": "var(--wp--preset--font-size--normal)",
				"font-family": "var(--wp--preset--font-family--base)"
			}
		}
	}
}
