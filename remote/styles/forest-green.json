{
	"version": 2,
	"title": "Forest Green",
	"settings": {
		"custom": {
			"button": {
				"typography": {
					"fontWeight": "700"
				},
				"hover": {
					"color": {
						"text": "var(--wp--preset--color--foreground)",
						"background": "var(--wp--preset--color--background)"
					},
					"border": {
						"color": "var(--wp--preset--color--foreground)"
					}
				}
			}
		},
		"color": {
			"palette": [
				{
					"slug": "primary",
					"color": "#064E49",
					"name": "Primary"
				},
				{
					"slug": "background",
					"color": "#FFF9F2",
					"name": "Background"
				},
				{
					"slug": "foreground",
					"color": "#064E49",
					"name": "Foreground"
				}
			],
			"duotone": [
				{
					"colors": [
						"#064E49",
						"#FFF9F2"
					],
					"slug": "default-filter",
					"name": "Default filter"
				}
			]
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFamily": "\"Crimson Pro\", sans-serif",
					"slug": "crimson-pro",
					"name": "Body (Crimson Pro)",
					"fontFace": [
						{
							"fontFamily": "Crimson Pro",
							"fontDisplay": "block",
							"fontWeight": "300",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [ "file:./assets/fonts/CrimsonPro-Light.ttf" ]
						},
						{
							"fontFamily": "Crimson Pro",
							"fontDisplay": "block",
							"fontWeight": "300",
							"fontStyle": "italic",
							"fontStretch": "normal",
							"src": [ "file:./assets/fonts/CrimsonPro-Italic.ttf" ]
						},
						{
							"fontFamily": "Crimson Pro",
							"fontDisplay": "block",
							"fontWeight": "700",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [ "file:./assets/fonts/CrimsonPro-Bold.ttf" ]
						},
						{
							"fontFamily": "Crimson Pro",
							"fontDisplay": "block",
							"fontWeight": "700",
							"fontStyle": "italic",
							"fontStretch": "normal",
							"src": [ "file:./assets/fonts/CrimsonPro-BoldItalic.ttf" ]
						}
					]
				}
			],
			"fontSizes": [
				{
					"name": "Small",
					"size": "1rem",
					"slug": "small"
				},
				{
					"name": "Normal",
					"size": "1.125rem",
					"slug": "normal"
				},
				{
					"name": "Medium",
					"size": "1.5rem",
					"slug": "medium"
				},
				{
					"name": "Large",
					"size": "1.75rem",
					"slug": "large"
				},
				{
					"name": "Huge",
					"size": "min(max(2rem, 5vw), 2.25rem)",
					"slug": "huge"
				}
			]
		}
	},
	"styles": {
		"blocks": {
			"core/site-title": {
				"elements": {
					"link": {
						"typography": {
							"fontWeight": "700"
						}
					}
				}
			},
			"core/button": {
				"typography": {
					"fontWeight": "var(--wp--custom--button--typography--font-weight)"
				}
			},
			"core/cover": {
				"filter": {
					"duotone": "var(--wp--preset--duotone--default-filter)"
				}
			},
			"core/image": {
				"filter": {
					"duotone": "var(--wp--preset--duotone--default-filter)"
				}
			},
			"core/post-featured-image": {
				"filter": {
					"duotone": "var(--wp--preset--duotone--default-filter)"
				}
			},
			"core/heading": {
				"typography": {
					"fontWeight": "700"
				}
			}
		},
		"elements": {
			"h6": {
				"typography": {
					"textTransform": "uppercase"
				}
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--crimson-pro)",
			"fontSize": "var(--wp--preset--font-size--normal)",
			"fontWeight": "300",
			"lineHeight": "1.5"
		}
	}
}
