{
	"$schema": "https://json.schemastore.org/theme-v1.json",
	"version": 1,
	"customTemplates": [
		{
			"name": "blank",
			"title": "Blank",
			"postTypes": [
				"page",
				"post"
			]
		},
		{
			"name": "header-footer-only",
			"title": "Header and Footer Only",
			"postTypes": [
				"page",
				"post"
			]
		}
	],
	"settings": {
		"color": {
			"palette": [
				{
					"slug": "primary",
					"color": "#333333",
					"name": "Primary"
				},
				{
					"slug": "secondary",
					"color": "#0087be",
					"name": "Secondary"
				},
				{
					"slug": "tertiary",
					"color": "#54ac65",
					"name": "Tertiary"
				},
				{
					"slug": "background",
					"color": "#ffffff",
					"name": "Background"
				},
				{
					"slug": "foreground",
					"color": "#767676",
					"name": "Foreground"
				}
			]
		},
		"typography": {
			"fontSizes": [
				{
					"name": "Small",
					"size": "15px",
					"slug": "small"
				},
				{
					"name": "Normal",
					"size": "19px",
					"slug": "normal"
				},
				{
					"name": "Medium",
					"size": "24px",
					"slug": "medium"
				},
				{
					"name": "Large",
					"size": "32px",
					"slug": "large"
				},
				{
					"name": "Huge",
					"size": "47px",
					"slug": "huge"
				}
			]
		},
		"custom": {
			"gap": {
				"baseline": "10px"
			}
		},
		"layout": {
			"contentSize": "740px",
			"wideSize": "1110px"
		}
	},
	"styles": {
		"typography": {
			"fontFamily": "Georgia, \"Times New Roman\", serif"
		},
		"blocks": {
			"core/post-title": {
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--custom--color--primary)"
						}
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--huge)"
				}
			},
			"core/site-tagline": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--system-font)",
					"fontSize": "17px"
				}
			},
			"core/site-title": {
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--custom--color--primary)"
						}
					}
				},
				"typography": {
					"fontSize": "38px"
				}
			}
		},
		"elements": {
			"h1": {
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--custom--color--primary)"
						}
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--huge)",
					"fontWeight": "700"
				}
			},
			"h2": {
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--custom--color--primary)"
						}
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)",
					"fontWeight": "700"
				}
			},
			"h3": {
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--custom--color--primary)"
						}
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontWeight": "700"
				}
			},
			"h4": {
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--custom--color--primary)"
						}
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--normal)",
					"fontWeight": "700"
				}
			},
			"h5": {
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--custom--color--primary)"
						}
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontWeight": "700"
				}
			},
			"h6": {
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--custom--color--primary)"
						}
					}
				},
				"typography": {
					"fontSize": "var(--wp--custom--font-sizes--tiny)",
					"fontWeight": "700"
				}
			}
		}
	}
}
