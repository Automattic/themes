	"settings": {
		"appearanceTools": true,
		"color": {
			"palette": [
				{
					"color": "#A19982",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#000000",
					"name": "Foreground",
					"slug": "foreground"
				},
				{
					"color": "#ffffff",
					"name": "Background",
					"slug": "background"
				},
				{
					"color": "#f1ede6",
					"name": "Tertiary",
					"slug": "tertiary"
				}
			]
		},
		"custom": {
			"gap": {
				"horizontal": "clamp(20px, 6vw, 100px)"
			}
		},
		"layout": {
			"contentSize": "820px",
			"wideSize": "1240px"
		},
		"spacing": {
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
