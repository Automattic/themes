			"palette": [
				{
					"color": "#131415",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#929292",
					"name": "Contrast",
					"slug": "contrast"
				},
				{
					"color": "#ffffff",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#6becae",
					"name": "Secondary",
					"slug": "secondary"
