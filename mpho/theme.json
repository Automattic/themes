						"max": "2rem",
						"min": "1.75rem"
					},
					"name": "Large",
					"size": "1.75rem",
					"slug": "large"
				},
				{
					"fluid": {
						"max": "3rem",
						"min": "2.5rem"
					},
					"name": "Extra Large",
					"size": "3rem",
					"slug": "x-large"
				}
			]
		},
		"useRootPaddingAwareAlignments": true
