				},
				{
					"colors": [
						"#728d36",
						"#F6F6E6"
					],
					"name": "Gecko",
					"slug": "gecko"
				},
				{
					"colors": [
						"#e08d68",
						"#F6F6E6"
					],
					"name": "Firewatch",
					"slug": "firewatch"
				},
				{
					"colors": [
						"#508f9b",
						"#F6F6E6"
					],
					"name": "Java",
					"slug": "java"
				},
				{
					"colors": [
						"#B4956C",
						"#F6F6E6"
					],
					"name": "Gold",
					"slug": "gold"
				}
			],
			"palette": [
				{
					"color": "#f6f6e6",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#111111",
					"name": "Contrast",
					"slug": "contrast"
