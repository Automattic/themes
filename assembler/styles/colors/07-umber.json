{
    "$schema": "https://schemas.wp.org/trunk/theme.json",
    "version": 3,
    "title": "Umber",
    "category": "professional",
    "keywords": [
        "earthy",
        "rustic",
        "warm",
        "sophisticated",
        "muted",
        "natural",
        "cozy",
        "elegant",
        "organic",
        "timeless",
        "ecofriendly",
        "artisanal",
        "brown"
    ],
    "settings": {
        "color": {
            "palette": [
                {
                    "color": "#f5f3f2",
                    "name": "Color 1",
                    "slug": "theme-1"
                },
                {
                    "color": "#131211",
                    "name": "Color 2",
                    "slug": "theme-2"
                },
                {
                    "color": "#cbc5bf",
                    "name": "Color 3",
                    "slug": "theme-3"
                },
                {
                    "color": "#685D4B",
                    "name": "Color 4",
                    "slug": "theme-4"
                }
            ],
            "duotone": [
                {
                    "colors": [
                        "#2A2317",
                        "#FFFFFF"
                    ],
                    "slug": "duotone-1",
                    "name": "Duotone 1"
                }
            ],
            "gradients": [
                {
                    "slug": "gradient-text-transparent",
                    "gradient": "linear-gradient(to bottom, var(--wp--preset--color--theme-2) -50%, transparent 50%)",
                    "name": "Text to Transparent"
                },
                {
                    "slug": "gradient-1-2",
                    "gradient": "linear-gradient(to bottom, var(--wp--preset--color--theme-1) 0%, var(--wp--preset--color--theme-2) 100%)",
                    "name": "Gradient 1"
                },
                {
                    "slug": "gradient-1-3",
                    "gradient": "linear-gradient(to bottom, var(--wp--preset--color--theme-1) 0%, var(--wp--preset--color--theme-3) 100%)",
                    "name": "Gradient 2"
                },
                {
                    "slug": "gradient-1-4",
                    "gradient": "linear-gradient(to bottom, var(--wp--preset--color--theme-1) 0%, var(--wp--preset--color--theme-4) 100%)",
                    "name": "Gradient 3"
                },
                {
                    "slug": "gradient-2-3",
                    "gradient": "linear-gradient(to bottom, var(--wp--preset--color--theme-2) 0%, var(--wp--preset--color--theme-3) 100%)",
                    "name": "Gradient 4"
                },
                {
                    "slug": "gradient-2-4",
                    "gradient": "linear-gradient(to bottom, var(--wp--preset--color--theme-2) 0%, var(--wp--preset--color--theme-4) 100%)",
                    "name": "Gradient 5"
                },
                {
                    "slug": "gradient-3-4",
                    "gradient": "linear-gradient(to bottom, var(--wp--preset--color--theme-3) 0%, var(--wp--preset--color--theme-4) 100%)",
                    "name": "Gradient 6"
                }
            ]
        }
    },
    "styles": {
        "color": {
            "text": "var(--wp--preset--color--theme-2)"
        },
        "blocks": {
            "core/separator": {
                "color": {
                    "text": "color-mix(in srgb, var(--wp--preset--color--theme-4) 20%, transparent)"
                }
            }
        },
        "elements": {
            "button": {
                ":hover": {
                    "color": {
                        "background": "color-mix(in srgb, var(--wp--preset--color--theme-4) 70%, #000)",
                        "text": "var(--wp--preset--color--theme-1)"
                    }
                },
                "color": {
                    "background": "var(--wp--preset--color--theme-4)",
                    "text": "var(--wp--preset--color--theme-1)"
                }
            }
        },
        "variations": {
            "section-1": {
                "color": {
                    "background": "var(--wp--preset--color--theme-3)",
                    "text": "var(--wp--preset--color--theme-2)"
                },
                "blocks": {
                    "core/separator": {
                        "color": {
                            "text": "color-mix(in srgb, var(--wp--preset--color--theme-4) 25%, transparent)"
                        }
                    }
                },
                "elements": {
                    "button": {
                        ":hover": {
                            "color": {
                                "background": "color-mix(in srgb, var(--wp--preset--color--theme-4) 70%, #000)",
                                "text": "var(--wp--preset--color--theme-1)"
                            }
                        },
                        "color": {
                            "background": "var(--wp--preset--color--theme-4)",
                            "text": "var(--wp--preset--color--theme-1)"
                        }
                    }
                }
            },
            "section-2": {
                "color": {
                    "background": "var(--wp--preset--color--theme-4)",
                    "text": "var(--wp--preset--color--theme-3)"
                },
                "blocks": {
                    "core/separator": {
                        "color": {
                            "text": "color-mix(in srgb, currentColor 25%, transparent)"
                        }
                    }
                },
                "elements": {
                    "button": {
                        "color": {
                            "background": "var(--wp--preset--color--theme-1)",
                            "text": "var(--wp--preset--color--theme-4)"
                        },
                        ":hover": {
                            "color": {
                                "background": "color-mix(in srgb, var(--wp--preset--color--theme-1) 85%, transparent)",
                                "text": "var(--wp--preset--color--theme-4)"
                            }
                        }
                    },
                    "heading": {
                        "color": {
                            "text": "var(--wp--preset--color--theme-1)"
                        }
                    }
                }
            },
            "section-3": {
                "color": {
                    "background": "var(--wp--preset--color--theme-2)",
                    "text": "var(--wp--preset--color--theme-3)"
                },
                "blocks": {
                    "core/separator": {
                        "color": {
                            "text": "color-mix(in srgb, currentColor 25%, transparent)"
                        }
                    }
                },
                "elements": {
                    "button": {
                        ":hover": {
                            "color": {
                                "background": "color-mix(in srgb, var(--wp--preset--color--theme-4) 70%, #000)",
                                "text": "var(--wp--preset--color--theme-1)"
                            }
                        },
                        "color": {
                            "background": "var(--wp--preset--color--theme-4)",
                            "text": "var(--wp--preset--color--theme-1)"
                        }
                    },
                    "heading": {
                        "color": {
                            "text": "var(--wp--preset--color--theme-1)"
                        }
                    }
                }
            }
        }
    }
}
