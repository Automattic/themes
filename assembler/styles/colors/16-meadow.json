    "styles": {
        "color": {
            "background": "var(--wp--preset--color--theme-1)",
            "text": "var(--wp--preset--color--theme-2)"
        },
        "elements": {
            "button": {
                "color": {
                    "background": "var(--wp--preset--color--theme-2)",
                    "text": "var(--wp--preset--color--theme-6)"
                },
                ":hover": {
                    "color": {
                        "background": "color-mix(in srgb, var(--wp--preset--color--theme-2) 75%, #000)",
                        "text": "var(--wp--preset--color--theme-6)"
                    }
                }
            }
        },
        "variations": {
            "section-1": {
                "color": {
                    "background": "var(--wp--preset--color--theme-3)",
                    "text": "var(--wp--preset--color--theme-2)"
                }
            },
            "section-2": {
                "color": {
                    "background": "var(--wp--preset--color--theme-4)",
                    "text": "var(--wp--preset--color--theme-2)"
                }
            },
            "section-3": {
                "color": {
                    "background": "var(--wp--preset--color--theme-2)",
                    "text": "var(--wp--preset--color--theme-1)"
                },
                "blocks": {
                    "core/separator": {
                        "color": {
                            "text": "currentColor"
                        }
                    }
                },
                "elements": {
                    "button": {
                        "color": {
                            "background": "var(--wp--preset--color--theme-1)",
                            "text": "var(--wp--preset--color--theme-2)"
                        },
                        ":hover": {
                            "color": {
