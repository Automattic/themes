            "core/button": {
                "variations": {
                    "outline": {
                        "border": {
                            "color": "var(--wp--preset--color--theme-1)"
                        },
                        "color": {
                            "text": "var(--wp--preset--color--theme-2)"
                        }
                    }
                }
            },
            "core/separator": {
                "border": {
                    "color": "var(--wp--preset--color--theme-1)"
                }
            }
        },
        "elements": {
            "heading": {
                "color": {
                    "text": "var(--wp--preset--color--theme-2)"
                }
            },
            "button": {
                "color": {
                    "background": "var(--wp--preset--color--theme-4)",
                    "text": "var(--wp--preset--color--theme-2)"
                },
                ":hover": {
                    "color": {
