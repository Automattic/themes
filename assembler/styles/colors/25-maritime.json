        "business",
        "professional",
        "corporate",
        "nautical",
        "trustworthy",
        "calm",
        "authoritative",
        "classic",
        "maritime",
        "finance",
        "light",
        "blue",
        "education",
        "academic",
        "non-profit",
        "knowledge"
