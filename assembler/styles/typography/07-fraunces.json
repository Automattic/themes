{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"title": "Fraunces & Inter",
	"category": "professional",
	"keywords": [
		"serif",
		"friendly",
		"charming",
		"inviting",
		"calm",
		"professional",
		"elegant",
		"versatile"
	],
	"settings": {
		"typography": {
			"fontSizes": [
				{
					"name": "Small",
					"size": "17px",
					"slug": "small",
					"fluid": {
						"min": "15px",
						"max": "17px"
					}
				},
				{
					"name": "Medium",
					"size": "26px",
					"slug": "medium",
					"fluid": {
						"min": "16px",
						"max": "26px"
					}
				},
				{
					"name": "Large",
					"size": "34px",
					"slug": "large",
					"fluid": {
						"min": "22px",
						"max": "34px"
					}
				},
				{
					"name": "Extra Large",
					"size": "64px",
					"slug": "x-large",
					"fluid": {
						"min": "47px",
						"max": "64px"
					}
				},
				{
					"name": "2X Large",
					"size": "95px",
					"slug": "xx-large",
					"fluid": {
						"min": "60px",
						"max": "95px"
					}
				}
			]
		}
	},
	"styles": {
		"blocks": {
			"core/site-title": {
				"typography": {
					"fontSize": "20px",
					"fontFamily": "var(--wp--preset--font-family--fraunces)",
					"fontWeight": "200"
				}
			},
			"core/navigation": {
				"typography": {
					"fontSize": "20px",
					"fontFamily": "var(--wp--preset--font-family--fraunces)",
					"fontWeight": "200"
				}
			}
		},
		"elements": {
			"button": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontWeight": "300"
				}
			},
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--fraunces)",
					"fontWeight": "200"
				}
			},
			"h1": {
				"typography": {
					"lineHeight": "0.9"
				}
			},
			"h2": {
				"typography": {
					"lineHeight": "0.9"
				}
			},
			"h3": {
				"typography": {
					"fontWeight": "300",
					"fontSize": "var(--wp--preset--font-size--large)"
				}
			},
			"h4": {
				"typography": {
					"fontWeight": "300",
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"h5": {
				"typography": {
					"fontWeight": "300",
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"h6": {
				"typography": {
					"fontWeight": "300",
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--inter)",
			"fontWeight": "300",
			"lineHeight": "1.6",
			"fontSize": "var(--wp--preset--font-size--small)"
		}
	}
}
