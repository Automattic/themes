{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"title": "Fraunces & Inter",
	"keywords": [
		"serif",
		"friendly",
		"charming",
		"inviting",
		"calm",
		"professional",
		"elegant",
		"versatile"
	],
	"settings": {
		"typography": {
			"fontSizes": [
				{
					"name": "Small",
					"size": "16px",
					"slug": "small"
				},
				{
					"name": "Medium",
					"size": "17px",
					"slug": "medium",
					"fluid": {
						"min": "15px",
						"max": "17px"
					}
				},
				{
					"name": "Large",
					"size": "40px",
					"slug": "large"
				},
				{
					"name": "Extra Large",
					"size": "60px",
					"slug": "x-large"
				},
				{
					"name": "2X Large",
					"size": "92px",
					"slug": "xx-large",
					"fluid": {
						"min": "40px",
						"max": "92px"
					}
				}
			]
		}
	},
	"styles": {
		"blocks": {
			"core/site-title": {
				"typography": {
					"fontSize": "20px",
					"fontFamily": "var(--wp--preset--font-family--fraunces)",
					"fontWeight": "200"
				}
			},
			"core/navigation": {
				"typography": {
					"fontSize": "20px",
					"fontFamily": "var(--wp--preset--font-family--fraunces)",
					"fontWeight": "200"
				}
			}
		},
		"elements": {
			"button": {
				"typography": {
					"fontSize": "18px",
					"fontWeight": "300"
				}
			},
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--fraunces)",
					"fontWeight": "200"
				}
			},
			"h1": {
				"typography": {
					"lineHeight": "1"
				}
			},
			"h2": {
				"typography": {
					"lineHeight": "1"
				}
			},
			"h3": {
				"typography": {
					"fontWeight": "300",
					"fontSize": "32px"
				}
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--inter)",
			"fontWeight": "300",
			"lineHeight": "1.6",
			"fontSize": "var(--wp--preset--font-size--medium)"
		}
	}
}
