{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"title": "Ibarra & Inter",
	"categories": [
	  "serif",
	  "traditional",
	  "formal",
	  "legible",
	  "business",
	  "professional"
	],
	"settings": {
		"typography": {
			"fontSizes": [
				{
					"name": "Small",
					"size": "19px",
					"slug": "small",
					"fluid": {
						"min": "16px",
						"max": "19px"
					}
				},
				{
					"name": "Medium",
					"size": "22px",
					"slug": "medium"
				},
				{
					"name": "Large",
					"size": "44px",
					"slug": "large"
				},
				{
					"name": "Extra Large",
					"size": "76px",
					"slug": "x-large",
					"fluid": {
						"min": "26px",
						"max": "76px"
					}
				},
				{
					"name": "2X Large",
					"size": "100px",
					"slug": "xx-large",
					"fluid": {
						"min": "40px",
						"max": "100px"
					}
				}
			]
		}
	},
	"styles": {
		"blocks": {
			"core/site-title": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--inter)",
					"fontSize": "13px",
					"fontWeight": "500",
					"textTransform": "uppercase",
					"letterSpacing": "2px"
				}
			},
			"core/navigation": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--inter)",
					"fontSize": "13px",
					"fontWeight": "500",
					"textTransform": "uppercase",
					"letterSpacing": "2px"
				}
			}
		},
		"elements": {
			"button": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--inter)",
					"fontSize": "13px",
					"fontWeight": "500",
					"textTransform": "uppercase",
					"letterSpacing": "2px"
				}
			},
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--ibarra-real-nova)",
					"fontWeight": "100",
					"lineHeight": "1"
				}
			},
			"h1": {
				"typography": {
					"lineHeight": "0.9"
				}
			},
			"h2": {
				"typography": {
					"lineHeight": "1"
				}
			},
			"h3": {
				"typography": {
					"lineHeight": "1.2"
				}
			},
			"h4": {
				"typography": {
					"lineHeight": "1.2"
				}
			},
			"h5": {
				"typography": {
					"lineHeight": "1.2"
				}
			},
			"h6": {
				"typography": {
					"lineHeight": "1.2"
				}
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--ibarra-real-nova)",
			"fontSize": "var(--wp--preset--font-size--small)",
			"fontStyle": "normal",
			"fontWeight": "400",
			"lineHeight": "1.75"
		}
	}
}
