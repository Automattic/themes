{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"title": "Lucette & DM Mono",
	"categories": [
		"serif",
		"elegant",
		"contemporary",
		"technical",
		"monospace"
	],
	"settings": {
		"typography": {
			"fontSizes": [
				{
					"name": "Small",
					"size": "15px",
					"slug": "small",
					"fluid": {
						"min": "14px",
						"max": "15px"
					}
				},
				{
					"name": "Medium",
					"size": "19px",
					"slug": "medium"
				},
				{
					"name": "Large",
					"size": "40px",
					"slug": "large"
				},
				{
					"name": "Extra Large",
					"size": "60px",
					"slug": "x-large"
				},
				{
					"name": "2X Large",
					"size": "85px",
					"slug": "xx-large"
				}
			]
		}
	},
	"styles": {
		"blocks": {
			"core/site-title": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontFamily": "var(--wp--preset--font-family--dm-mono)",
					"fontWeight": "300"
				}
			},
			"core/navigation": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontFamily": "var(--wp--preset--font-family--dm-mono)",
					"fontWeight": "300"
				}
			}
		},
		"elements": {
			"button": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--dm-mono)",
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontWeight": "400"
				}
			},
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--lucette)",
					"fontWeight": "normal"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "28px"
				}
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--dm-mono)",
			"lineHeight": "1.75",
			"fontSize": "var(--wp--preset--font-size--small)",
			"fontWeight": "300"
		}
	}
}
