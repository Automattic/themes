			"palette": [
				{
					"color": "#ffffff",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#242121",
					"name": "Contrast",
					"slug": "contrast"
				},
				{
					"color": "#242121",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#6e6e6e",
					"name": "Secondary",
					"slug": "secondary"
