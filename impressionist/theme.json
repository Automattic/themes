			"fontSizes": [
				{
					"fluid": {
						"max": "0.9rem",
						"min": "0.8rem"
					},
					"name": "Extra Small",
					"size": "0.8rem",
					"slug": "x-small"
				},
				{
					"fluid": {
						"max": "1.2rem",
						"min": "1rem"
					},
					"name": "Small",
					"size": "1rem",
					"slug": "small"
				},
				{
					"fluid": {
						"max": "1.4rem",
						"min": "0.9rem"
					},
					"name": "Medium",
					"size": "1.4rem",
					"slug": "medium"
				},
				{
					"fluid": true,
					"name": "Large",
					"size": "2.1rem",
					"slug": "large"
				},
				{
					"fluid": true,
					"name": "Extra Large",
					"size": "2.52rem",
					"slug": "x-large"
				},
				{
					"fluid": true,
					"name": "Double Extra Large",
					"size": "3.024rem",
					"slug": "xx-large"
				},
				{
					"fluid": {
						"max": "7.525rem",
						"min": "2.1rem"
					},
					"name": "Huge",
					"size": "7.525rem",
					"slug": "huge"
				}
			]
		},
		"useRootPaddingAwareAlignments": true
