					"text": "var(--wp--preset--color--secondary)"
				},
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--secondary)"
						}
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-small)",
					"letterSpacing": "1.4px",
					"textTransform": "uppercase"
				}
			},
			"core/post-author-name": {
				"elements": {
					"link": {
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						},
						"typography": {
							"textDecoration": "none"
						}
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-small)"
