				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-small)",
					"letterSpacing": "1.4px",
					"textTransform": "uppercase"
				},
				"variations": {
					"outline": {
						"border": {
							"bottom": {
								"color": "var(--wp--preset--color--contrast)",
								"style": "solid",
								"width": "1px"
							},
							"left": {
								"color": "var(--wp--preset--color--contrast)",
								"style": "solid",
								"width": "1px"
							},
							"radius": "100px",
							"right": {
								"color": "var(--wp--preset--color--contrast)",
								"style": "solid",
								"width": "1px"
							},
							"top": {
