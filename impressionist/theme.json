					"name": "System Font",
					"slug": "system-font"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Beth Ellen",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/beth-ellen_normal_400.ttf"
							]
						}
					],
					"fontFamily": "Beth Ellen",
					"slug": "beth-ellen"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Cardo",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/cardo_normal_400.ttf"
