						"color": {
							"text": "var(--wp--preset--color--secondary)"
						}
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--cardo)",
					"fontSize": "var(--wp--preset--font-size--small)",
					"lineHeight": "1.6"
				}
			},
			"core/separator": {
				"border": {
					"color": "currentColor",
